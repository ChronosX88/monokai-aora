	"tokenColors": [
		/** COMMENTS */
		{
			"name": "Comment",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "#ABABAB",
				"fontStyle": ""
			}
		},
		{
			"name": "Comment annotations",
			"scope": [
				"storage.type.class.jsdoc"
			],
			"settings": {
				"foreground": "#19D1E5",
				"fontStyle": ""
			}
		},
		{
			"name": "Comment variable",
